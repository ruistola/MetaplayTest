{
  "extends": "@metaplay/typescript-config/tsconfig.json",
  "compilerOptions": {
    "lib": [
      "ESNext",
      "DOM"
    ],
    "noEmit": true,
    "outDir": "../../../.moon/cache/types/MetaplaySDK/Frontend/MetaUiNext",
    "types": [
      "vite/client"
    ]
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.vue",
    ".storybook/*.ts"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "vueCompilerOptions": {
    "plugins": [
      "@vue/language-plugin-pug"
    ]
  }
}
