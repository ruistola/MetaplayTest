dotnet_diagnostic.CA1050.severity = warning # Declare types in namespaces
dotnet_diagnostic.CA1067.severity = warning # Override Equals when implementing IEquatable
dotnet_diagnostic.CA1069.severity = warning # Enums should not have duplicate values
dotnet_diagnostic.CA1304.severity = warning # Specify CultureInfo
dotnet_diagnostic.CA1305.severity = warning # Specify IFormatProvider
dotnet_diagnostic.CA1310.severity = warning # Specify StringComparison for correctness
dotnet_diagnostic.CA1806.severity = warning # Do not ignore method results
dotnet_diagnostic.CA2208.severity = warning # Instantiate argument exceptions correctly
dotnet_diagnostic.CA2215.severity = warning # Dispose methods should call base class dispose

dotnet_diagnostic.RS2008.severity = none # Enable analyzer release tracking
dotnet_diagnostic.VSTHRD200.severity = none # Use "Async" suffix in names of methods that return an awaitable type (disabled for now as causes lots of noise)
dotnet_diagnostic.VSTHRD003.severity = none # Avoid awaiting or returning a Task representing work that was not started within your context as that can lead to deadlocks. (disabled, inane)
dotnet_diagnostic.VSTHRD103.severity = none # Dispose synchronously blocks. Await DisposeAsync instead. (DisposeAsync is often not available on Unity and hence often not actionable. May be re-enabled in the future)

# C# XML comment diagnostics: we only care about some kinds of warnings, and disable the rest.
# Currently we mostly care about warnings that decrease the usefulness of comments in the IDE, such as invalid crefs or ill-formed XML.
dotnet_diagnostic.CS1591.severity = none # Missing XML comment for publicly visible type or member
dotnet_diagnostic.CS1573.severity = none # Parameter 'myParam' has no matching param tag in the XML comment for 'MyMethod(...)' (but other parameters do)
dotnet_diagnostic.CS1712.severity = none # Type parameter 'TMyTypeParam' has no matching typeparam tag in the XML comment on 'MyThing<...>' (but other type parameters do)
dotnet_diagnostic.CS0419.severity = none # Ambiguous reference in cref attribute
dotnet_diagnostic.CS1723.severity = none # XML comment has cref attribute 'TMyTypeParam' that refers to a type parameter

# Disable locale warnings about StringBuilder.Append (and AppendLine) with an interpolated string expression.
# Without this exclusion, CA1305 would warn about Append with a interpolated string expression even if it only
# contains string-typed parts. We assume that plain strings are never formatted in a locale-specific way.
# Note that this does silence also legitimate warnings about Append usage, i.e. those involving interpolated
# string expressions with parts formatted in a locale-specific way, but the custom MP_STR_00 is intended to
# catch those.
dotnet_code_quality.CA1305.excluded_symbol_names = M:System.Text.StringBuilder.Append(System.Text.StringBuilder.AppendInterpolatedStringHandler@)|M:System.Text.StringBuilder.AppendLine(System.Text.StringBuilder.AppendInterpolatedStringHandler@)

# Metaplay custom code analyzers
dotnet_diagnostic.MP_STR_00.severity = warning # Default-formatted interpolated string
dotnet_diagnostic.MP_STR_01.severity = warning # Default-formatted object-to-string addition
dotnet_diagnostic.MP_STR_02.severity = warning # Default-formatted ToString()
dotnet_diagnostic.MP_STR_03.severity = warning # Default-formatted method invocation
dotnet_diagnostic.MP_STR_04.severity = warning # Default-comparing string ordering
dotnet_diagnostic.MP_STR_05.severity = warning # Prefer Ordinal string comparison
dotnet_diagnostic.MP_VAR_00.severity = warning # Local variable is never read
dotnet_diagnostic.MP_STI_00.severity = warning # Struct has an explicit parameterless instance constructor
dotnet_diagnostic.MP_STI_01.severity = warning # Struct instance member has an initializer
dotnet_diagnostic.MP_CLC_00.severity = warning # API controller has mismatching logger category name
dotnet_diagnostic.MP_CON_00.severity = warning # Async lambda is converted to void-returning delegate


